# App
APP_ENV=development
FRONTEND_URL=http://localhost:3000

# Supabase
SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
SUPABASE_JWT_SECRET=YOUR_SUPABASE_JWT_SECRET

# OpenAI
OPENAI_API_KEY=YOUR_OPENAI_API_KEY
OPENAI_MODEL=gpt-4o-mini

# Optional pricing (USD per 1K tokens). Used only to estimate cost in usage_events.
OPENAI_PRICE_INPUT_PER_1K=0.00015
OPENAI_PRICE_OUTPUT_PER_1K=0.00060

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PRICE_ID_PRO=price_...

# Checkout redirects
STRIPE_SUCCESS_URL=http://localhost:3000/app/billing?success=1
STRIPE_CANCEL_URL=http://localhost:3000/app/billing?canceled=1

# Limits / Retention
FREE_DAILY_ANALYZE_LIMIT=1
PRO_DAILY_ANALYZE_LIMIT=10
FREE_REPORT_RETENTION_DAYS=3
PRO_REPORT_RETENTION_DAYS=30

