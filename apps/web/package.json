{
  "name": "routineiq-web",
  "private": true,
  "scripts": {
    "dev": "node -e \"require('fs').rmSync('.next', { recursive: true, force: true })\" && next dev -p 3000",
    "env:check": "node scripts/check-env.mjs",
    "build": "npm run env:check && node scripts/clean-next.mjs && next build",
    "start": "next start -p 3000",
    "start:e2e": "next start -p 3100",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --incremental false",
    "test:e2e": "playwright test",
    "test:e2e:live": "E2E_MODE=live playwright test e2e/core-flows.spec.ts -g \"F2\"",
    "test:e2e:headed": "playwright test --headed"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.45.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "next": "^14.2.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.13.3",
    "swr": "^2.4.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/node": "^20.17.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.18",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3"
  }
}
